<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.poly.test.naversearchstudy">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_baseline_menu_book_24"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_baseline_menu_book_24"
        android:supportsRtl="true"
        android:theme="@style/Theme.NaverSearchStudy">
        <activity android:name=".recyclerview.BookResultActivity"></activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>